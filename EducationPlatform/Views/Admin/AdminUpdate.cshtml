
@{
    ViewBag.Title = "AdminUpdate";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Instituition Update</title>
</head>
<body>
    <table class=" table table-dark">
        <form action="" method="post">
        <tbody>

            <tr>
                <td>Id:</td>
                <td>@Model.Id</td>
            </tr>
            <tr>
                <td>Name:</td>
                <td>
                    <input type="text" name="Name" value="@(Model.Name)" class="form-control" />
                    <!--@Html.ValidationMessage("Name") -->
                </td>
            </tr>
            <tr>

                <td>Address</td>
                <td><input type="text" name="Address" value="@(Model.Address)" class="form-control" /></td>
            </tr>
            <tr>
                <td>Email:</td>

                <td><input type="text" name="Email" value="@(Model.Email)" class="form-control" /></td>
            </tr>
            <tr>
                <td>Phone:</td>

                <td><input type="text" name="Phone" value="@(Model.Phone)" class="form-control" /></td>
            </tr>
            <tr>
                <td>Password:</td>

                <td><input type="text" name="Password" value="@(Model.Password)" class="form-control" /></td>
            </tr>
            
            <tr>
                <td></td>
                <td><input type="submit" class="btn btn-success" /></td>
            </tr>





        </tbody>

        </form>


    </table>
</body>
</html>



