
@{
    ViewBag.Title = "TransactionHistory";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Instituition List</title>
    <!--CSS bundle Link-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <table class=" table table-dark">
        <thead>
            <tr>
                <th class="bg-warning" scope="col">ID</th>
                <th class="bg-warning" scope="col">Student ID</th>
                <th class="bg-warning" scope="col">Course Id</th>
                <th class="bg-warning" scope="col">Institute Id</th>
                <th class="bg-warning" scope="col">Bkash Transaction Number</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model)
            {
                <tr>
                    <th scope="row">@i.Id</th>
                    <td>
                        @i.StudentId
                        <br />
                        <a href="~/AdminStudent/SingleStudentList/@i.StudentId" class="btn btn-success">See Details</a>
                    </td>
                    <td>
                        @i.CourseId
                        <br />
                        <a href="~/AdminStudent/SingleStudentList/@i.CourseId" class="btn btn-success">See Details</a>
                    </td>
                   
                    <td>
                        @i.InstitutionId
                        <br />
                        <a href="~/AdminStudent/SingleStudentList/@i.InstitutionId" class="btn btn-success">See Details</a>
                    </td>
                    <td>@i.BkashTransactionNumber</td>
                   
                </tr>
            }

        </tbody>
    </table>







    <!--JS bundle Link-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>

