
@{
    ViewBag.Title = "InstitutionRevenue";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Instituition List</title>
    <!--CSS bundle Link-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <section class="container mt-5">
        <div>
            <table class=" table table-dark">

                <tbody>

                    <tr>
                        <td>Name:</td>
                        <td>
                            @ViewBag.varsityName
                        </td>
                    </tr>

                    <tr>
                        <td>Total Sold Course:</td>
                        <td>
                            @ViewBag.totalSellingCourse
                        </td>
                    </tr>
                    <tr>
                        <td>Total Selling:</td>
                        <td>
                            @ViewBag.sellingAmount
                        </td>
                    </tr>
                    <tr>
                        <td>Your Earning:</td>
                        <td>
                            @ViewBag.myEarning
                        </td>
                    </tr>
                    <tr>
                        <td>Varsity Earning:</td>
                        <td>
                            @ViewBag.varsityEarning
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <a href="~/AdminRevenue/InstitutionRevenueReportPdf/@ViewBag.institutionId" class="btn btn-success">Download Report</a>

                        </td>
                    </tr>


                </tbody>
            </table>
        </div>
    </section>


    <!--JS bundle Link-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>


